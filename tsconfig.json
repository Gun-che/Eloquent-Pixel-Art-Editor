{
  "compilerOptions": {
    "sourceMap": true,
    "outDir": "./docs/",
    "module": "es6",
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "skipLibCheck": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    // "noEmit": true,
    "downlevelIteration": true
  },
  "files": ["./index.ts"],
  "include": ["src", "index.ts", "dist/keyListener.ts"]
}
